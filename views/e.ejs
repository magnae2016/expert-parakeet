            width: 162px;
            border: 1px solid #ecf0f2;
            border-left: 0;
            border-top: 0;
            border-bottom: 0;
        }

        .table_header table {
            width: 100%;
            table-layout: fixed;
        }

        .table_header table th {
            height: 42px;
            background-color: #f8fafb;
            vertical-align: middle;
            text-align: center;
            color: #666;
            font-size: 14px;
            font-weight: normal;
        }

        .table_header table tr+tr th {
            border-top: 1px solid #ecf0f2;
        }

        .table_body {
            overflow: hidden;
            overflow-x: auto;
        }

