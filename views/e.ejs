        }

        .sub_option_area .sub_option::before {
            content: "";
            display: inline-block;
            width: 5px;
            height: 5px;
            margin-right: 6px;
            -webkit-border-radius: 3px;
            border-radius: 3px;
            -webkit-box-sizing: border-box;
            box-sizing: border-box;
            background-color: #9e9e9e;
            vertical-align: 3px;
        }

        .sub_option_area .sub_option[aria-selected="true"] {
            color: #242424;
        }

        .sub_option_area .sub_option[aria-selected="true"]::before {
            background-color: #0abe16;
        }

        .table_content {
            overflow: hidden;
        }

